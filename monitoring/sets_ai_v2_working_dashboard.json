                    }
                  }
                },
                "plotType": "LINE",
                "targetAxis": "Y2",
                "legendTemplate": "5xx Errors/min"
              }
            ],
            "yAxis": {
              "label": "Requests/min",
              "scale": "LINEAR"
            },
            "y2Axis": {
              "label": "Errors/min",
              "scale": "LINEAR"
            }
          }
        }
      },
      {
        "yPos": 11,
        "width": 4,
        "height": 4,
